# Backend API URL
REACT_APP_API_URL=http://localhost:5000/api

# Clerk Authentication (Frontend Key)
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_test_your-clerk-publishable-key-here

# Google Maps API Key (Same as backend)
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Development Configuration
REACT_APP_ENV=development

# Optional: Analytics (for production)
# REACT_APP_GA_TRACKING_ID=your_google_analytics_id

# Optional: Sentry for error tracking (for production)
# REACT_APP_SENTRY_DSN=your_sentry_dsn

# Optional: Cloudinary for image uploads (for future)
# REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloudinary_name
# REACT_APP_CLOUDINARY_UPLOAD_PRESET=your_upload_preset